{
   "env": {
      "browser": true,
      "es2022": true
   },
   "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "airbnb",
      "prettier"
   ],
   "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "ecmaFeatures": {
         "jsx": true
      }
   },
   "plugins": ["react"],
   "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
   }
}
